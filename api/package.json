{
  "name": "kps-api",
  "version": "1.0.0",
  "description": "KPS Pest Control Management System - Backend API",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "start": "node app.js",
    "start:prod": "NODE_ENV=production node app.js",
    "start:standalone": "node app-standalone.js",
    "dev": "nodemon src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "deploy": "npm run build",
    "cpanel:deploy": "npm run build && echo 'Built successfully for cPanel deployment. Upload files to your cPanel hosting.'",
    "cpanel:restart": "mkdir -p tmp && touch tmp/restart.txt && echo 'Created restart.txt to restart Passenger application'"
  },
  "keywords": [
    "pest-control",
    "api",
    "nodejs",
    "express",
    "mysql",
    "typescript"
  ],
  "author": "KPS Development Team",
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/nodemailer": "^7.0.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "html-pdf-node": "^1.0.8",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "web-push": "^3.6.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/html-pdf-node": "^1.0.2",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
